eval homedir="$(printf "~/%q" "$relativepath")"
touch ~/.bash_machine_specific
declare -a includeFiles=(
	$homedir".bash_aliases"
	$homedir".bash_completion"
	$homedir".bash_functions"
	$homedir".bash_paths"
	$homedir".bash_machine_specific"
)

for i in "${includeFiles[@]}"
do
	if [ -f $i ]; then
		. $i;
	fi;
done


